schema tag {

    document tag extends base {

        field tagid type int {
            indexing: summary | attribute
        }

        field score type int {
            indexing: summary | attribute
        }

    }

    rank-profile default inherits default {
        first-phase {
            expression: attribute(score)
        }
    }

}
